{
  "functions": {
    "api/generate-concept.js": {
      "maxDuration": 30
    },
    "api/generate-image.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}